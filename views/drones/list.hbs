<h2>Available drones</h2>
{{#each drones}}
<div>
  <p><strong>Name: </strong>{{ this.name }}</p>
  <p><strong>Propellers: </strong>{{ this.propellers }}</p>
  <p><strong>Max Speed: </strong>{{ this.maxSpeed }}</p>
  <form action="/drones/{{ this.id }}/edit" method="GET">
    <button type="submit">Edit</button>
  </form>
</div>
<br />
{{/each}}
